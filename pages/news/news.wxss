/**index.wxss**/
page {
  height: 100%;
  width: 100%;
}

.custom-image {
   height: 50rpx;
   width: 200rpx;
}

.indexPageLodingNotice {
   text-align: center;
}
.example-search {
 background-color: #fff;
 padding: 16rpx 32rpx;
 margin-top: 5px;
 width: 93%;
 margin-bottom: 10px;
}
.hurdle{
 width: 100%;
 padding-top: 40rpx;
 padding-bottom: 40rpx;
 background-color:rgb(234, 238, 238) ;
 margin-bottom: 20rpx;
}
.group {
 width: 90%;
 height: 450rpx;
 margin: 0 auto;
 border-radius: 20rpx;
 margin-top: 5rpx;
 box-shadow: 0 0 3rpx #000;
}
.image{
 width: 100%;
 height: 390rpx;
 float: left;
 border-top-left-radius: 20rpx;
 border-top-right-radius: 20rpx;
}
.top-time{
 width: 100%;
 height: 40rpx;
 float: left;
 height: 58rpx;
 border-bottom-left-radius: 20rpx;
 border-bottom-right-radius: 20rpx;
}
.low{
 width: 100%;
 height: 60rpx;
 margin-top:330rpx;
 background-color: rgb(0, 0, 0,0.5);
}
.container{
 display: flex;
 flex-direction: column;
 height: 100%;
}

.nav{
 width: 100%;
 height: 80rpx;
 background-color: skyblue;
 display: flex;
 flex-direction: row;
 justify-content: space-around;
}

.item{
 font-size: 32rpx;
 color: #000;
 height: 50rpx;
 /* border: tomato solid 1rpx; */
}

.active{
border-bottom: tomato solid 7rpx;
}

.list{
 width: 100%;
 height: 100%;
 display: flex;
}