.page {
    height: 100vh;
    display: flex;
    flex-direction: column;
}

.custom-image {
    height: 50rpx;
    width: 200rpx;
}

.indexPageLodingNotice {
    text-align: center;
}

.alumniPage {
    margin-left: 40rpx;
    margin-right: 40rpx;
}

.schoolInfoCard {
    height: 300rpx;
    padding: 40rpx;
    background: linear-gradient(135deg, #d388ee, #268ba1);
    border-radius: 40rpx;
    box-shadow: 2rpx 2rpx 20rpx #efefef;
    display: flex;
    align-items: center;
    justify-content: space-between;

}

.custom-image-container {
    position: relative;
    width: 380rpx;
    height: 360rpx;
    margin-right: 90rpx;
    border-radius: 20%;
    overflow: hidden;
    box-shadow: 1px 1px 10px #f3f3f3;
    display: flex;
    justify-content: space-between;
    align-items: center;
}


.custom-USYimage {
    width: 380rpx;
    height: 360rpx;
    margin-left: auto;
    border-radius: 10%;
}

.images-container {
    display: flex;
    flex-direction: column;
    margin-top: -100rpx;
}

.applyforId {
    width: 100rpx;
    height: 100rpx;
    margin-top: 60rpx;
}

.registerforId {
    width: 100rpx;
    height: 100rpx;
    margin-top: 40rpx;
}

.textSize {
    font-size: 24rpx;
    margin-top: -40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}





.functionalComponent {
    margin-top: 10px;
    padding-top: 5px;
    background: linear-gradient(135deg, #d5f1f4, #9dc3fb);
    width: auto;
    height: 660rpx;
    display: flex;
    flex-wrap: wrap;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}


